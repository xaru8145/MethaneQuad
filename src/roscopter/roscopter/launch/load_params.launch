<launch>
  <node name="$(anon dynparam)" pkg="dynamic_reconfigure" type="dynparam" args="set_from_parameters controller_node">

    <param name="x_dot_P" type="double" value="0.1" />
    <param name="x_dot_I" type="double" value="0.0" />
    <param name="x_dot_D" type="double" value="0.0" /> # induces jumpiness
    
    <param name="y_dot_P" type="double" value="0.1" />
    <param name="y_dot_I" type="double" value="0.0" />
    <param name="y_dot_D" type="double" value="0.0" /> # induces jumpiness
    
    <param name="z_dot_P" type="double" value="0.25" />
    <param name="z_dot_I" type="double" value="0.0" />
    <param name="z_dot_D" type="double" value="0.0" /> # induces jumpiness
    
    <param name="north_P" type="double" value="0.3" />
    <param name="north_I" type="double" value="0.05" />
    <param name="north_D" type="double" value="0.0" />
    
    <param name="east_P" type="double" value="0.3" />
    <param name="east_I" type="double" value="0.05" />
    <param name="east_D" type="double" value="0.0" />
    
    <param name="down_P" type="double" value="1.0" />
    <param name="down_I" type="double" value="0.2" />
    <param name="down_D" type="double" value="0.0" />
    
    <param name="psi_P" type="double" value="1.0" />
    <param name="psi_I" type="double" value="0.0" />
    <param name="psi_D" type="double" value="0.0" />
    
    <param name="tau" type="double" value="0.05" />
    <param name="equilibrium_throttle" type="double" value="0.54" />

    <param name="max_roll" type="double" value="0.196" />
    <param name="max_pitch" type="double" value="0.196" />
    <param name="max_yaw_rate" type="double" value="0.25" />
    <param name="max_throttle" type="double" value=".85" />
    <param name="max_n_dot" type="double" value="1.0" /> <!-- 1.5 -->
    <param name="max_e_dot" type="double" value="1.0" /> <!-- 1.5 -->
    <param name="max_d_dot" type="double" value="1.0" /> <!-- 0.5 -->
  </node>
</launch>
